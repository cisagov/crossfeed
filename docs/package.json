{
  "name": "crossfeed-docs",
  "description": "Crossfeed Documentation",
  "version": "1.0.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "engineStrict": true,
  "scripts": {
    "clean": "rimraf .cache public _site",
    "build": "gatsby build",
    "reset": "npx rimraf .git",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "lint": "prettier --check \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "codegen": "swagger-jsdoc -d ./src/swaggerDef.js -o ./src/generated/swagger.json",
    "postinstall": "npx npm-force-resolutions && npm run codegen"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.13",
    "classnames": "^2.2.6",
    "eslint-config-react-app": "^5.2.1",
    "gatsby": "^5.9",
    "gatsby-cli": "^5.9",
    "gatsby-plugin-manifest": "^5.9",
    "gatsby-plugin-meta-redirect": "^1.1.1",
    "gatsby-plugin-react-helmet": "^6.8.0",
    "gatsby-plugin-sass": "^6.8.0",
    "gatsby-plugin-sharp": "^5.9",
    "gatsby-remark-autolink-headers": "^6.8.0",
    "gatsby-remark-images": "^7.8.0",
    "gatsby-remark-prismjs": "^7.8.0",
    "gatsby-source-filesystem": "^5.9",
    "gatsby-transformer-remark": "^6.9.0",
    "html-loader": "^4.2.0",
    "prettier": "^2.1.2",
    "prismjs": "^1.27.0",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "rimraf": "^3.0.2",
    "sass": "^1.61.0",
    "swagger-jsdoc": "^6.2",
    "swagger-ui-react": "^4.18.2",
    "uswds": "^2.13.3"
  },
  "resolutions": {
    "trim": "0.0.3",
    "sanitize-html": "2.10.0",
    "ssri": "8.0.1",
    "parse-url": "8.1.0"
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.21.3",
    "@reach/router": "^1.3.4",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "clipboardy": "^3.0.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0"
  },
  "overrides": {
    "clipboardy": {
      "execa": "5.1.1"
    },
    "gatsby": {
      "cache-manager": "^5.2.1"
    }
  }
}
