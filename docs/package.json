{
  "name": "crossfeed-docs",
  "description": "Crossfeed Documentation",
  "version": "1.0.0",
  "engines": {
    "node": ">=16.0.0 <17.0.0"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "lint": "prettier --check \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "codegen": "swagger-jsdoc -d ./src/swaggerDef.js -o ./src/generated/swagger.json",
    "postinstall": "npx npm-force-resolutions && npm run codegen"
  },
  "dependencies": {
    "btoa": "^1.2.1",
    "postcss": "^8.4.19"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.13",
    "classnames": "^2.2.6",
    "eslint-config-react-app": "^5.2.1",
    "gatsby": "^5.2.0",
    "gatsby-plugin-manifest": "^5.2.0",
    "gatsby-plugin-meta-redirect": "^1.1.1",
    "gatsby-plugin-react-helmet": "^6.2.0",
    "gatsby-plugin-sass": "^6.2.0",
    "gatsby-plugin-sharp": "^5.2.0",
    "gatsby-remark-autolink-headers": "^6.2.0",
    "gatsby-remark-images": "^7.2.0",
    "gatsby-remark-prismjs": "^7.2.0",
    "gatsby-source-filesystem": "^5.2.0",
    "gatsby-transformer-remark": "^6.2.0",
    "html-loader": "^1.3.2",
    "prettier": "^2.1.2",
    "prismjs": "^1.27.0",
    "prop-types": "^15.7.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-helmet": "^6.1.0",
    "sass": "^1.29.0",
    "swagger-jsdoc": "^5.0.1",
    "swagger-ui-react": "^4.15.5",
    "uswds": "^2.9.0"
  },
  "resolutions": {
    "postcss": "8.4.19",
    "trim": "0.0.3",
    "sanitize-html": "2.3.2",
    "ssri": "8.0.1",
    "parse-url": "8.1.0",
    "loader-utils": "3.2.1",
    "engine.io": "6.2.1"
  }
}
