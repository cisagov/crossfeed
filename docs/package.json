{
  "name": "crossfeed-docs",
  "description": "Crossfeed Documentation",
  "version": "1.0.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "engineStrict": true,
  "scripts": {
    "clean": "rimraf .cache public _site",
    "build": "gatsby build",
    "reset": "npx rimraf .git",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "lint": "prettier --check \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "codegen": "swagger-jsdoc -d ./src/swaggerDef.js -o ./src/generated/swagger.json",
    "postinstall": "npm run codegen"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "classnames": "^2.2.6",
    "eslint-config-react-app": "^7.0.1",
    "gatsby": "^5.11.0",
    "gatsby-cli": "^5.11.0",
    "gatsby-plugin-manifest": "^5.11.0",
    "gatsby-plugin-meta-redirect": "^1.1.1",
    "gatsby-plugin-react-helmet": "^6.11.0",
    "gatsby-plugin-sass": "^6.11.0",
    "gatsby-plugin-sharp": "^5.11.0",
    "gatsby-remark-autolink-headers": "^6.11.0",
    "gatsby-remark-images": "^7.11.0",
    "gatsby-remark-prismjs": "^7.11.0",
    "gatsby-source-filesystem": "^5.11",
    "gatsby-transformer-remark": "^6.11.0",
    "html-loader": "^4.2.0",
    "prettier": "^2.1.2",
    "prismjs": "^1.27.0",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "rimraf": "^3.0.2",
    "sass": "^1.63.6",
    "swagger-ui-react": "^5.1.0",
    "uswds": "^2.14"
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.22.7",
    "@reach/router": "^1.3.4",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "clipboardy": "^3.0.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "resolve-url-loader": "^5.0.0",
    "swagger-jsdoc": "^6.2.8"
  },
  "overrides": {
    "@typescript-eslint/eslint-plugin": "$@typescript-eslint/eslint-plugin",
    "@typescript-eslint/parser": "$@typescript-eslint/parser",
    "babel-eslint": "npm:@babel/eslint-parser",
    "clipboardy": {
      "execa": "5.1.1"
    },
    "css-minimizer-webpack-plugin": "^5.0.0",
    "engine.io": "6.4.2",
    "gatsby": {
      "cache-manager": "^5.2.1"
    },
    "react": "$react",
    "react-dom": "$react-dom",
    "resolve-url-loader": "$resolve-url-loader",
    "simple-get": "^4.0.1",
    "socket.io": "4.6.1",
    "yaml": "^2.2.2",
    "semver": "^7.5.3"
  }
}